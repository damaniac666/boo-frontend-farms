    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^7.16.0",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "source-map-explorer": "^2.5.1",
    "vite": "^7.2.2"
  }
